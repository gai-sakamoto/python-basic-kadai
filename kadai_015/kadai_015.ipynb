        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}\")\n",
        "        print(f\"年齢: {self.age}\")\n",
        "\n",
        "# インスタンスの作成\n",
        "person = Human(\"山田太郎\", 25)\n",
        "\n",
        "# メソッドの呼び出し\n",
        "person.printinfo()"
      ]
    }
